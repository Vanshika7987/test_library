<p><%= notice %></p>
<h1>listing users</h1>
<%= link_to 'new user', new_user_path%>

</div>
<table>
<thead>
<tr>
<th>name</th>
<th>role</th>
<th>phone_number</th>
<th colspan ="3">Actions</th>
</tr>
</thead>
<tbody>
<% @users.each do|user| %>
<tr>
<td><%=user.name %></td>
<td><%=user.role %></td>
<td><%=user.phone_number %></td>
<td><%= link_to 'view', user %></td>
<td><%= link_to 'edit', edit_user_path(user) %></td>
<td><%= link_to 'delete', user,  method: :delete, data: {confirm: 'are you sure?'} %></td>
</tr>
<% end %>
</tbody>
</table>
<%= paginate @users%>